vsphere_user="<vsphere-login-user>"
vsphere_password="<vsphere-login-password>"
vsphere_server="<vsphere-server-ipaddress>"
datacenter="<vsphere-datacenter-name>"
datastore=["<datastore1>", "<datastore2>", "<datastore3>"]
resource_pool="<cluster-name>/Resources"
network="VM Network"                            #Replace with your Virtua Switch
template = {                                    #VM Template list in vSphere:
    "redhat"="rhel74_base"                      #Template value use key/value pair: "<key>"="<template-name-in-vsphere>"
    "ubuntu"="ubuntu1604_base"
}
storage_driver = {                              #Docker Storage Driver. Keep the default value: redhat: devicemapper, ubuntu: overlay2
    "redhat"="devicemapper"                      
    "ubuntu"="overlay2"
}
osfamily="ubuntu"                               #Use for lookup template and storage driver: possible value: ubuntu, redhat
ssh_user="ubuntu-user"                          #Login user in selected template
ssh_password="your-password"                    #Login password in selected template
vm_domain="domain.local"                        #OS Domain
dns_list = ["x.x.x.x"]                          #DNS Servers
timezone = "Asia/Singapore"                     #OS Time Zone
vm_private_key_file="vmware-key"                #Private Key Filename to be saved on local
icp_version="2.1.0.3-ee"                        #ICP Version #Community: 2.1.0.3 #Enterprise: 2.1.0.3-ee , ibmcom/icp-inception-amd64:3.1.0-ee
#icp_source_server="x.x.x.x"                    #SFTP Server for Enterprise Edition
#icp_source_user="ftp-user"                     #SFTP Login user
#icp_source_password="ftp-password"             #SFTP Login password
#icp_source_path="path-to-icp-enterprise-file"  #Download file location in SFTP server
#cluster_vip="x.x.x.x"                          #ICP Cluster Virtual IP for HA Deployment (Enterprise edition)
cluster_vip_iface="ens160"                      #ICP Cluster Virtual IP Interface name for HA. #Ubuntu: ens160 #RHEL: ens192 #Default: eth0
#proxy_vip="192.168.66.94"                      #ICP Proxy Virtual IP for HA Deployment (Enterprise edition)
proxy_vip_iface="ens160"                        #ICP Proxy Virtual IP Interface name for HA. #Ubuntu: ens160 #RHEL: ens192 #Default: eth0
cluster_name="mycluster"                        #ICP Cluster Name
instance_prefix="icp"                           #VM Instance name prefix
install_gluster=true                            #Option to install GlusterFS
gluster_volume_type = "none"                    #Gluster Volume Type. Sample: none, replicate:3, disperse:4:2
heketi_admin_pwd = "your-password"              #Heketi API admin password
disable_management = ["istio", "custom-metrics-adapter"] #Default Disabled management component v2.1.0.3
master= {                                       #VM Specification for ICP Master node
    nodes       = "1"
    name        = "master"
    cpu_cores   = "4"
    kubelet_lv  = "10"                          #Kubelet Volume size
    docker_lv   = "50"                          #Docker Volume size
    etcd_lv     = "4"                           #Etcd Volume size
    registry_lv = "35"                          #Registry Volume size
    management_lv = "100"                       #Management Volume size
    memory      = "16384"
    ipaddresses = "192.168.1.81"                #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"
}
proxy = {
    nodes       = "1"
    name        = "proxy"
    cpu_cores   = "2"
    kubelet_lv  = "10"
    docker_lv   = "39"
    memory      = "4096"
    ipaddresses = "192.168.1.84"                #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"    
}
management = {
    nodes       = "1"
    name        = "mgmt"
    cpu_cores   = "4"
    kubelet_lv  = "10"
    docker_lv   = "39"
    management_lv = "100"                       #Management Volume size
    memory      = "8192"
    ipaddresses = "192.168.1.87"                #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"    
}  
va = {
    nodes       = "1"
    name        = "va"
    cpu_cores   = "4"
    kubelet_lv  = "10"
    docker_lv   = "39"
    va_lv       = "100"                       #Management Volume size
    memory      = "8192"
    ipaddresses = "192.168.1.88"              #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"    
}
worker = {
    nodes       = "3"
    name        = "worker"
    cpu_cores   = "8"
    kubelet_lv  = "10"
    docker_lv   = "89"
    memory      = "8192"
    ipaddresses = "192.168.1.90,192.168.1.91,192.168.1.92"   #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"
}
gluster= {
    nodes       = "3"
    name        = "gluster"
    cpu_cores   = "2"
    data_disk   = "100" // GB                               #Data disk for Gluster
    memory      = "2048"
    ipaddresses = "192.168.1.95,192.168.1.96,192.168.1.97"  #Comma separated IP Addressed for more than one node
    netmask     = "24"
    gateway     = "192.168.1.1"
}